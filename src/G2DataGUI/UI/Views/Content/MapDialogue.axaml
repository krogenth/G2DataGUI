			 xmlns:unknown10Opcodes="using:G2DataGUI.Common.Data.Maps.MapDialogueOpcodes.Unknown10"
             mc:Ignorable="d"
             Focusable="True"
			 x:DataType="vm:MapDialogueViewModel">
	<UserControl.Resources>
		<selectors:MapOpcodeSelector x:Key="OpcodeTemplate">
