						      DataType="{x:Type opcodes:CameraMoveOpcode}">
				<StackPanel>
					<TextBlock Text="Opcode:" />
					<TextBox Text="{Binding Opcode}" />
					<TextBlock Text="Unknown1:" />
					<TextBox Text="{Binding Unknown1}" />
					<TextBlock Text="Unknown2:" />
					<TextBox Text="{Binding Unknown2}" />
				</StackPanel>
			</TreeDataTemplate>

