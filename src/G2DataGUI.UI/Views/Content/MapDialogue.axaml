	<Grid Name="MapDialoguePage"
		  Margin="20,10">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="Auto" />
			<ColumnDefinition Width="*" />
		</Grid.ColumnDefinitions>
		<ListBox ItemsSource="{Binding SelectedMapDialogueOpcodes}"
				 x:Name="MapDialogueOpcodeList"
				 SelectedIndex="{Binding SelectedMapDialogueOpcodeIndex}"
				 Grid.Column="0">
			<ListBox.Styles>
				<Style Selector="ListBoxItem">
					<Setter Property="Width" Value="175" />
					<Setter Property="HorizontalAlignment" Value="Right" />
				</Style>
			</ListBox.Styles>
			<ListBox.ItemTemplate>
				<DataTemplate>
					<Border CornerRadius="4"
							Margin="0"
							Padding="10"
							HorizontalAlignment="Stretch"
							VerticalAlignment="Stretch"
							ClipToBounds="True">
						<TextBlock>
							<TextBlock.Text>
								<MultiBinding Converter="{StaticResource ListElementToIndexConverter}">
									<MultiBinding.Bindings>
										<Binding Path="$parent[ListBox].Items" />
										<Binding Path="$parent[ListBoxItem].DataContext" />
									</MultiBinding.Bindings>
								</MultiBinding>
							</TextBlock.Text>
						</TextBlock>
					</Border>
				</DataTemplate>
			</ListBox.ItemTemplate>
		</ListBox>
		<TreeView Grid.Column="1"
				  ItemsSource="{Binding SelectedMapDialogueOpcodeItem}"
				  ItemTemplate="{DynamicResource OpcodeTemplate}">
			<TreeView.ItemsPanel>
				<ItemsPanelTemplate>
					<VirtualizingStackPanel />
				</ItemsPanelTemplate>
			</TreeView.ItemsPanel>
		</TreeView>
	</Grid>
</UserControl>
